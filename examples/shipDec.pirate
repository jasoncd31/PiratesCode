ship Rectangle {
    build (int h, int w){
        int me.height = h
        int me.width = w
    }

    captain area() -> int {
        anchor me.width * me.height
    }

    captain getHeight() -> int {
        anchor me.height
    }

    captain getWidth() -> int {
        anchor me.width
    }
}

vargh sign = new Rectangle(5,5)
ahoy sign.area()
