ship Rectangle {
    build (int height, int weight){
        int me.height = height
        int me.width = width
    }

    captain area(){
        anchor me.width * me.height
    }

    captain getHeight(){
        anchor me.height
    }

    captain getWidth(){
        anchor me.width
    }
}

vargh sign = new Rectangle(5,5)
ahoy sign.area()